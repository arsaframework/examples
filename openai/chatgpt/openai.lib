{
	set($openai_id, curl(\"-X POST -H "Content-Type: application/json" -H "Authorization: Bearer $token" -d '{  "model": "text-davinci-003",  "prompt": "$question", "max_tokens": 1000, "temperature": 1.0 }' https://api.openai.com/v1/completions"\) )
